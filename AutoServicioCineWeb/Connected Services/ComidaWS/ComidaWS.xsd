<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://WS.autoservicio.autoservicio.cinepucp.com.pe/" targetNamespace="http://WS.autoservicio.autoservicio.cinepucp.com.pe/" version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
