    <xs:sequence>
      <xs:element minOccurs="0" name="return" type="tns:cupon" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="cupon">
    <xs:sequence>
      <xs:element name="activo" type="xs:boolean" />
